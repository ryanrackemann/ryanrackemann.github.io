{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/login",
      "allowedRoles": ["anonymous"],
      "redirect": "/.auth/login/github"
    },
    {
      "route": "/*",
      "allowedRoles": ["authenticated"]
    }
  ],
  "responseOverrides": {
    "401": { "redirect": "/login", "statusCode": 302 }
  }
}
